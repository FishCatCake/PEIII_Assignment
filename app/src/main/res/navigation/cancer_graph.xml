<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cancer_graph"
    app:startDestination="@id/cancerCalendarFragment">

    <fragment
        android:id="@+id/cancerCalendarFragment"
        android:name="com.example.pe_assignment.cancer.CancerCalendarFragment"
        android:label="fragment_cancer_canlendar"
        tools:layout="@layout/fragment_cancer_canlendar" >
        <action
            android:id="@+id/action_cancerCalendarFragment_to_cancerTimelineFragment"
            app:destination="@id/cancerTimelineFragment" />
        <action
            android:id="@+id/action_cancerCalendarFragment_to_cancerCreateRecordActivity"
            app:destination="@id/cancerCreateRecordActivity" />
    </fragment>
    <fragment
        android:id="@+id/cancerTimelineFragment"
        android:name="com.example.pe_assignment.cancer.CancerTimelineFragment"
        android:label="fragment_cancer_timeline"
        tools:layout="@layout/fragment_cancer_timeline" >
        <action
            android:id="@+id/action_cancerTimelineFragment_to_cancerCalendarFragment"
            app:destination="@id/cancerCalendarFragment" />
        <action
            android:id="@+id/action_cancerTimelineFragment_to_newTimelineActivity"
            app:destination="@id/newTimelineActivity" />
    </fragment>
    <activity
        android:id="@+id/cancerCreateRecordActivity"
        android:name="com.example.pe_assignment.CancerCreateRecordActivity"
        android:label="activity_cancer_create_record"
        tools:layout="@layout/activity_cancer_create_record" />
    <activity
        android:id="@+id/newTimelineActivity"
        android:name="com.example.pe_assignment.NewTimelineActivity"
        android:label="activity_new_timeline"
        tools:layout="@layout/activity_new_timeline" />
</navigation>